{
   "name": "ts-typeorm-starter",
   "version": "0.0.1",
   "description": "Starter template including TS, Express, TypeORM, and JWT auth",
   "scripts": {
      "tsc": "tsc",
      "test": "jest",
      "start": "set debug=* && ts-node-dev --require dotenv/config  --respawn --transpileOnly ./src/index.ts",
      "prod": "tsc && node ./build/app.js",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-validator": "^0.11.0",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "helmet": "^3.21.2",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.19",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.22"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.1",
      "@types/cors": "^2.8.6",
      "@types/helmet": "0.0.45",
      "@types/jest": "^24.0.25",
      "@types/jsonwebtoken": "^8.3.5",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^2.14.0",
      "@typescript-eslint/parser": "^2.14.0",
      "eslint": "^6.8.0",
      "eslint-config-prettier": "^6.9.0",
      "eslint-plugin-prettier": "^3.1.2",
      "jest": "^24.9.0",
      "prettier": "^1.19.1",
      "ts-jest": "^24.2.0",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.0.0-pre.44",
      "typescript": "3.3.3333"
   }
}
